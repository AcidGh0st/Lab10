            </Button>
            <Button layoutX="198.0" layoutY="113.0" mnemonicParsing="false" onAction="#bfsOnAction" prefHeight="32.0" prefWidth="133.0" text="BFS Tour">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <TextArea fx:id="txtArea" editable="false" layoutX="15.0" layoutY="217.0" prefHeight="343.0" prefWidth="362.0" />
