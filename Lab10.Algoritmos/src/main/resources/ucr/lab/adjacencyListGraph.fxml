            <Button layoutX="32.0" layoutY="69.0" mnemonicParsing="false" onAction="#containsEdgeOnAction" prefHeight="32.0" prefWidth="133.0" text="Contains Edge">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button layoutX="32.0" layoutY="26.0" mnemonicParsing="false" onAction="#randomizeOnAction" prefHeight="32.0" prefWidth="133.0" text="Randomize">
